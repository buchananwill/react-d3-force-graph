{
  "name": "react-d3-force-graph",
  "version": "0.1.0",
  "description": "A wrapper to use d3-forces in React projects.",
  "author": "Will Buchanan <willwritescode@gmail.com",
  "repository": {
    "type": "git",
    "url": "git+"
  },
  "keywords": [
    "react",
    "state",
    "utility"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist/**/*"
  ],
  "sideEffects": false,
  "scripts": {
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "build": "tsc",
    "lint": "eslint --fix src",
    "validate": "npm run lint && npm run coverage && npm run build && npm run size",
    "size": "size-limit"
  },
  "size-limit": [
    {
      "path": "dist/index.js"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "@types/d3": "^7.4.3",
    "d3": "^7.9.0",
    "lodash": "^4.17.21"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "selective-context": "2.0.1"
  },
  "devDependencies": {
    "react-dom": "^18.3.1",
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-react": "^7.24.1",
    "@types/lodash": "^4.17.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "clsx": "^2.1.1",
    "dependency-cruiser": "16.3.2",
    "eslint": "^9.2.0",
    "eslint-plugin-n": "^17.7.0",
    "eslint-plugin-vitest": "^0.5.4",
    "jsdom": "^24.0.0",
    "react": "^18.3.1",
    "prettier": "^3.2.5",
    "type-fest": "^4.18.1",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.8.0",
    "selective-context": "2.0.1"
  }
}
